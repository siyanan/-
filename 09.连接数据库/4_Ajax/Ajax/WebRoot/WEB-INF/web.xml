<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>JSServlet</servlet-name>
    <servlet-class>action.JSServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>jQueryServlet</servlet-name>
    <servlet-class>action.jQueryServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>JSServlet</servlet-name>
    <url-pattern>/JSServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>jQueryServlet</servlet-name>
    <url-pattern>/jQueryServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
